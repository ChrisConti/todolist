{
  "expo": {
    "name": "tribubaby",
    "slug": "todolist",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/baby.png",
      "resizeMode": "contain",
      "backgroundColor": "#C75B4A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tribubaby.tribubaby",
      "buildNumber": "4"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/baby.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tribubaby.tribubaby",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e46fab65-1809-4d8c-95c1-ac62f86ffef3"
      }
    },
    "plugins": [
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "tribubaby",
          "organization": "tribubaby"
        }
      ],
      "@react-native-firebase/app"
    ]
  }
}
